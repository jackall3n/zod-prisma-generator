{
  "name": "zod-prisma-generator",
  "version": "0.2.5",
  "type": "module",
  "bin": {
    "zod-prisma-generator": "dist/bin.mjs"
  },
  "license": "MIT",
  "description": "A Prisma generator that automatically creates Zod schemas from your Prisma schema models",
  "homepage": "https://github.com/jackall3n/zod-prisma-generator#readme",
  "bugs": {
    "url": "https://github.com/jackall3n/zod-prisma-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackall3n/zod-prisma-generator.git"
  },
  "author": "jackall3n <jackall3n@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./bin": "./dist/bin.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "bump": "bumpp",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@prisma/generator-helper": "^6.19.0",
    "@types/node": "^24.10.0",
    "bumpp": "^10.3.1",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
